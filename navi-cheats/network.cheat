% network

# Scp from remote to remote
scp <from_user>@<from_host>:<from_path> <to_user>@<to_host>:<to_path>

# Scp from local to remote
scp <from_path> <to_user>@<to_host>:<to_path>

# Scp from remote to local
scp <from_user>@<from_host>:<from_path> <to_path>

# Make dns request using nslookup
nslookup -q=<type> <name> <server>
$ type: for i in "any" "ns" "mx" "txt"; do echo $i; done

# Make dns request using host
host <name> <server>

# Scan ports using nmap
nmap -sS -T4 -A -n <target>
